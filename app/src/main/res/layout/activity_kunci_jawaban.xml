        android:id="@+id/button11" />

    <Button
        android:text="LEVEL 4"
        android:background="#fdd9ca94"
        android:textColor="#FFFFFF"
        android:textSize="15dp"
        android:textStyle="italic"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_below="@+id/button11"
        android:layout_alignLeft="@+id/button11"
        android:layout_alignStart="@+id/button11"
        android:layout_marginTop="29dp"
        android:id="@+id/button12" />

    <Button
        android:text="LEVEL 5"
        android:background="#fdd9ca94"
        android:textColor="#FFFFFF"
        android:textSize="15dp"
        android:textStyle="italic"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_below="@+id/button12"
        android:layout_alignLeft="@+id/button12"
        android:layout_alignStart="@+id/button12"
        android:layout_marginTop="27dp"
        android:id="@+id/button13" />

    <TextView
        android:text="Good Luck!^^"
        android:textColor="#daac9951"
        android:textSize="20dp"
        android:textStyle="italic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button13"
        android:layout_alignLeft="@+id/button13"
        android:layout_alignStart="@+id/button13"
        android:layout_marginTop="31dp"
        android:id="@+id/textView21" />

    <Button
        android:text="LEVEL 1"
        android:background="#fdd9ca94"
        android:textColor="#FFFFFF"
        android:textSize="15dp"
        android:textStyle="italic"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_marginTop="21dp"
        android:id="@+id/button8"
        android:layout_below="@+id/textView16"
        android:layout_alignLeft="@+id/textView16"
        android:layout_alignStart="@+id/textView16" />
</RelativeLayout>
